import stdlib.arow
import gpio.arow

bool send(int gpio_num, uint8 size, uint8 data) {
	uint8 i = size
	while (i > 0) {
		i = i - 1
		if (data & (1 << i) == 0) {
			gpioWrite(gpio_num, 0)
		}else{
			gpioWrite(gpio_num, 1)
		}
		wait(10)
	}
	return true
}

uint8 read() {
	gpioMode(2, 0)
	send(2, 2, 2)
	send(2, 1, 0)
	return 0
}
